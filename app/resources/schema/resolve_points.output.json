{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "ref": { "type": "string" },
          "code": { "type": "string" },
          "name": { "type": "string" }
        },
        "required": ["code", "name"],
        "additionalProperties": false
      }
    },
    "failed": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "index": { "type": "integer" },
          "ref": { "type": "string" },
          "code": { "type": "string", "enum": ["OUT_OF_RANGE", "INVALID_ARGUMENT"] }
        },
        "required": ["index", "code"],
        "additionalProperties": false
      }
    },
    "attribution": { "type": "string" }
  },
  "required": ["results", "failed", "attribution"],
  "additionalProperties": false
}
